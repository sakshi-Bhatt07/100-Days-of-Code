Q148: Take two structs as input and check if they are identical.

/*
Sample Test Cases:
Input 1:
Student1: Asha 101 90
Student2: Asha 101 90
Output 1:
Same

*/
 #include <stdio.h>
#include <string.h>

// Define Student structure
typedef struct {
    char name[50];
    int roll_no;
    int marks;
} Student;

// Function to compare two students
int areStudentsEqual(Student s1, Student s2) {
    if (strcmp(s1.name, s2.name) == 0 &&
        s1.roll_no == s2.roll_no &&
        s1.marks == s2.marks) {
        return 1;  // Same
    }
    return 0;  // Different
}

int main() {
    Student s1, s2;

    // Input Student 1
    printf("Enter details for Student 1:\n");
    printf("Name: ");
    scanf("%49s", s1.name);
    printf("Roll: ");
    scanf("%d", &s1.roll_no);
    printf("Marks: ");
    scanf("%d", &s1.marks);

    // Input Student 2
    printf("\nEnter details for Student 2:\n");
    printf("Name: ");
    scanf("%49s", s2.name);
    printf("Roll: ");
    scanf("%d", &s2.roll_no);
    printf("Marks: ");
    scanf("%d", &s2.marks);

    // Compare students
    if
 
