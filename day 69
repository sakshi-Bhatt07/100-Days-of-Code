//Q119: Write a program to take an integer array as input. Only one element will be repeated. Print the repeated element. Try to find the result in one single iteration.#include <stdio.h>

int main() {
    int n;
    printf("Enter size of array: ");
    scanf("%d", &n);

    int nums[n];
    printf("Enter array elements: ");
    for (int i = 0; i < n; i++)
        scanf("%d", &nums[i]);

    int visited[100000] = {0}; // assuming input values are small positive ints

    // Single iteration
    for (int i = 0; i < n; i++) {
        if (visited[nums[i]] == 1) {   // already seen
            printf("%d", nums[i]);
            return 0;
        }
        visited[nums[i]] = 1;
    }

    // Ideally never reaches here because one element is always repeated
    printf("No repetition");
    return 0;
}
